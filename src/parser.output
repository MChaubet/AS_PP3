    0 $accept: file $end

    1 file: declarations_globales body
    2     | body

    3 declarations_globales: declaration ';' declarations_globales
    4                      | declaration ';'

    5 declaration: LET names '=' functions
    6            | LET REC names '=' functions

    7 names: NAME names
    8      | NAME

    9 functions: FUNC names "=>" functions
   10          | expr

   11 declaration_locale: LET NAME '=' factor_expr IN expr
   12                   | LET REC NAME '=' factor_expr IN expr
   13                   | factor_expr WHERE NAME '=' expr
   14                   | factor_expr WHERE REC NAME '=' expr

   15 expr: conditional_expr
   16     | declaration_locale
   17     | match_expr

   18 conditional_expr: IF logical_expr THEN expr ELSE expr
   19                 | logical_expr

   20 logical_expr: logical_expr "||" relational_expr
   21             | logical_expr "&&" relational_expr
   22             | relational_expr

   23 relational_expr: relational_expr "<" additive_expr
   24                | relational_expr ">" additive_expr
   25                | relational_expr ">=" additive_expr
   26                | relational_expr "<=" additive_expr
   27                | relational_expr "==" additive_expr
   28                | relational_expr "!=" additive_expr
   29                | additive_expr

   30 additive_expr: additive_expr '+' multiplicative_expr
   31              | additive_expr '-' multiplicative_expr
   32              | multiplicative_expr

   33 multiplicative_expr: multiplicative_expr '*' negative_expr
   34                    | multiplicative_expr '/' negative_expr
   35                    | negative_expr

   36 negative_expr: factor_expr
   37              | '!' factor_expr

   38 factor_expr: '(' expr ')'
   39            | '"' text '"'
   40            | tree
   41            | NUMBER
   42            | NAME
   43            | EMIT_T '"' MOT '"' tree

   44 match_expr: MATCH_T expr WITH filter_expr END

   45 filter_expr: '|' pforests "=>" expr filter_expr
   46            | '|' pforests "=>" expr

   47 pforests: pattern_expr pforests
   48         | pattern_expr

   49 pattern_expr: '_'
   50             | TAG '{' pattern_expr '}'
   51             | TAG '{' '}'
   52             | '_' '{' pattern_expr '}'
   53             | '_' '{' '}'
   54             | '*' '_' '*'
   55             | '/' '_' '/'
   56             | '-' '_' '-'
   57             | NAME
   58             | '*' NAME '*'
   59             | '/' NAME '/'
   60             | '-' NAME '-'

   61 body: factor_expr body
   62     | factor_expr

   63 tree: TAG '[' attributs ']' '{' body '}'
   64     | TAG '[' attributs ']' '/'
   65     | TAG '{' body '}'
   66     | TAG '/'
   67     | '{' body '}'

   68 attributs: TAG '=' '"' text '"' attributs
   69          | TAG '=' '"' text '"'

   70 text: word_t text
   71     | word_t

   72 word_t: MOT ' '
   73       | MOT


